{
  "name": "@perfsee/iri",
  "version": "1.8.0",
  "description": "Proxy server with http2/https support",
  "private": "true",
  "keywords": [
    "Rust",
    "N-API",
    "napi-rs",
    "Proxy"
  ],
  "license": "UNLICENSED",
  "main": "index.js",
  "types": "./index.d.ts",
  "napi": {
    "name": "iri",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-unknown-linux-gnu"
      ]
    }
  },
  "scripts": {
    "build:rs": "napi build --release --platform",
    "debug:rs": "napi build --platform"
  },
  "dependencies": {
    "@node-rs/helper": "^1.2.1"
  },
  "devDependencies": {
    "@napi-rs/cli": "^1.3.3"
  }
}
